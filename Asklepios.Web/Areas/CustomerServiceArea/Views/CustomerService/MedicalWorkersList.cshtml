@model Asklepios.Web.Areas.CustomerServiceArea.Models.MedicalWorkersViewModel
    @{
        ViewData["Title"] = "View";
    }

    <h1 style="text-align:center">Lista pracowników medycznych</h1>

    @for (int i = 0; i < Model.MedicalWorkers.Count; i++)
    {
        MedicalWorker med = Model.MedicalWorkers[i];
        Asklepios.Web.Areas.CustomerServiceArea.Models.MedicalWorkerViewModel model = new Asklepios.Web.Areas.CustomerServiceArea.Models.MedicalWorkerViewModel(Model.SelectedPatient, med);
        //model.MedicalWorker = med;
        //model.SelectedPatient = Model.SelectedPatient;
        <partial name="_MedicalWorker" model="model" />

    }
